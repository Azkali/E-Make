{
  "name": "front",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --public-host www.e-make.io --port $(ts-node -pe 'require(`../cross/config/environments/local/common`).config.front.port')",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:prod": "ng build --prod",
    "build:prod+docker": "npm run build:prod && npm run docker:build",
    "build:preprod": "ng build --configuration=preprod",
    "build:preprod+docker": "npm run build:preprod && npm run docker:build",
    "docker:build": "docker image build -t gerkin/e-make-front .",
    "docker:tag": "docker tag gerkin/e-make-front gerkin/e-make-front:latest && docker tag gerkin/e-make-front gerkin/e-make-front:$(node -p 'require(`./package.json`).version')",
    "docker:push": "docker push gerkin/e-make-front:latest && docker push gerkin/e-make-front:$(node -p 'require(`./package.json`).version')"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.8",
    "@angular/cdk": "^7.3.3",
    "@angular/common": "^7.2.8",
    "@angular/compiler": "^7.2.8",
    "@angular/core": "^7.2.8",
    "@angular/forms": "^7.2.8",
    "@angular/http": "^7.2.8",
    "@angular/material": "^7.3.3",
    "@angular/platform-browser": "^7.2.8",
    "@angular/platform-browser-dynamic": "^7.2.8",
    "@angular/router": "^7.2.8",
    "@diaspora/diaspora": "git+https://github.com/diaspora-orm/diaspora.git#prerelease",
    "@diaspora/webapi-provider-oauth2": "0.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.1.0",
    "angular-oauth2-oidc": "^5.0.2",
    "angular2-cookie-law": "^7.0.1",
    "broadcast-channel": "^2.1.9",
    "core-js": "^2.6.5",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "ngx-showdown": "^4.0.2",
    "rxjs": "^6.4.0",
    "rxjs-compat": "^6.4.0",
    "showdown": "^1.9.0",
    "stack-subject": "^0.2.1",
    "tslint-angular": "^1.1.2",
    "zone.js": "^0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.5",
    "@angular/cli": "~7.3.5",
    "@angular/compiler-cli": "^7.2.8",
    "@angular/language-service": "^7.2.8",
    "@types/google.analytics": "0.0.39",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/lodash": "^4.14.122",
    "@types/node": "~11.11.0",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "~8.0.3",
    "tslint": "~5.13.1",
    "typescript": "<3.3.0"
  }
}
